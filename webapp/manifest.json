{
    "_version": "1.16.0",
    "sap.app": {
        "id": "ui5.walkthrough",
        "type": "application",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "UI5 Walkthrough",
        "description": "A simple UI5 application",
        "dataSources": {
            "mainService": {
                "uri": "/path/to/your/service",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "theme": "sap_fiori_3"
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "ui5.walkthrough.view.Main",
            "type": "XML",
            "async": true
        },
        "dependencies": {
            "minUI5Version": "1.60.0",
            "libs": {
                "sap.m": {}
            }
        }
    }
}